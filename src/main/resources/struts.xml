<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
  
<struts>
  
  <constant name="struts.devMode" value="true" />
  <constant name="struts.objectFactory" value="spring" />
  
  <package name="default"  extends="json-default">
  
    <action name="details" class="com.uneegohs.product.action.CollectEmail" method="gatheringEmailId">
      <result name="success"   >/planmain.jsp</result>
      <result name="failure">/main.jsp</result> 
         </action>
    
   <action name="getplans" class="com.uneegohs.product.action.DisplayPlans" method="displayPlans">
      <result name="success" >/product.jsp</result>
      <result name="failure">/noplans.jsp</result>
    </action>
    
    
   
    
     <action name="addcart" class="com.uneegohs.product.action.AddToCart" method="execute">
      <result name="success"  type = "redirect">http://www.google.com</result>
      <result name="failure"  type="redirect">http://ec2-18-219-227-199.us-east-2.compute.amazonaws.com:8080/order.jsp</result>
    </action>
     
  </package>
</struts>